{
  "title": "RimlHP",
  "name": "rimlhp",
  "author": "Daiki Takahashi",
  "description": "RimlことDaiki Takahashiのホームページです。",
  "version": "0.1.0",
  "license": "MIT License",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export && next-sitemap --config sitemap.config.js",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "style": "stylelint 'src/**/*.{css,scss}'",
    "style:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "prettier": "prettier --write --ignore-path .ignore './src/*.{js,jsx,ts,tsx,json,css,scss}'",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "npm": "当プロジェクトではnpmではなくyarnを利用してください。"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@storybook/addon-a11y": "^6.4.4",
    "@storybook/addon-toolbars": "^6.4.4",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "storybook-addon-performance": "^0.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@storybook/addon-actions": "^6.4.4",
    "@storybook/addon-essentials": "^6.4.4",
    "@storybook/addon-links": "^6.4.4",
    "@storybook/react": "^6.4.4",
    "@types/gtag.js": "^0.0.8",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "babel-loader": "^8.2.3",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "feed": "^4.2.2",
    "framer-motion": "^4",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "marked": "^4.0.6",
    "next-sitemap": "^1.6.203",
    "prettier": "^2.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "4.5.2"
  }
}
