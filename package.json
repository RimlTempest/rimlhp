{
    "name": "rimlhp",
    "author": "Daiki Takahashi",
    "description": "RimlことDaiki Takahashiのホームページです。",
    "version": "0.1.0",
    "license": "MIT License",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "prettier": "prettier --write --ignore-path .ignore './src/*.{js,jsx,ts,tsx,json,css,scss}'",
        "prepare": "husky install"
    },
    "engines": {
        "npm": "当プロジェクトではnpmではなくyarnを利用してください。"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "next lint --fix"
        ]
    },
    "dependencies": {
        "next": "12.0.4",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@chakra-ui/icons": "^1.1.1",
        "@chakra-ui/react": "^1.7.2",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "@types/node": "16.11.11",
        "@types/react": "17.0.37",
        "eslint": "7.32.0",
        "eslint-config-next": "12.0.4",
        "eslint-config-prettier": "^8.3.0",
        "framer-motion": "^4",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "prettier": "^2.5.0",
        "typescript": "4.5.2"
    }
}
