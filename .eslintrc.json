{
    "extends": ["next", "next/core-web-vitals", "prettier"],
    "rules": {
        "semi": ["error", "always"],
        "no-var": "error",
        "import/order": [
            "error",
            {
                "alphabetize": {
                    "order": "asc"
                }
            }
        ]
    },
    "ignorePatterns": ["*.config.js"]
}
